# Server Settings
PORT=3040
NODE_ENV=development

# API Keys
AILEVELUP_ENTERPRISE_API_KEY=your_ailevelup_enterprise_api_key_here
AILEVELUP_API_KEY=your_ailevelup_api_key_here
AILEVELUP_API_URL=https://your-api-endpoint.ailevelup.ai
STRIPE_SECRET_KEY=your_stripe_secret_key_here
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret_here
OPENAI_API_KEY=your_openai_api_key_here

# Twilio Settings
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here

# JWT 
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRATION=24h

# Supabase Settings
SUPABASE_URL=your_supabase_url_here
SUPABASE_SERVICE_KEY=your_supabase_service_key_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Google Sheets - Extract these from your service account JSON file
# For GOOGLE_SHEETS_PRIVATE_KEY, copy the entire private key including 
# BEGIN PRIVATE KEY and END PRIVATE KEY lines with all newline characters
GOOGLE_SHEETS_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYour private key here\n-----END PRIVATE KEY-----\n"
GOOGLE_SHEETS_CLIENT_EMAIL=your_service_account_email@example.iam.gserviceaccount.com
GOOGLE_SHEETS_DOC_ID=your_google_sheets_document_id

# AWS Settings
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here

# Database Configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=ailevelup_mcp
POSTGRES_USER=your_postgres_user
POSTGRES_PASSWORD=your_postgres_password

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=your_redis_password

# Logging Configuration
LOG_LEVEL=info
LOG_FORMAT=json

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Voice Configuration
DEFAULT_VOICE=alloy
DEFAULT_MODEL=turbo
DEFAULT_TEMPERATURE=0.7
DEFAULT_FROM_NUMBER=+15555555555

# Feature Flags
ENABLE_AUTO_TOPUP=false
ENABLE_WEBHOOKS=false
ENABLE_ANALYTICS=true 