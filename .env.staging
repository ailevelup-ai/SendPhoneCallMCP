# Server Settings
PORT=3040
NODE_ENV=staging
BYPASS_MCP_AUTH=false

# API Keys - Use staging credentials
AILEVELUP_ENTERPRISE_API_KEY=${STAGING_AILEVELUP_ENTERPRISE_API_KEY}
AILEVELUP_API_KEY=${STAGING_AILEVELUP_API_KEY}
AILEVELUP_API_URL=https://api.bland.ai
STRIPE_SECRET_KEY=${STAGING_STRIPE_SECRET_KEY}
STRIPE_WEBHOOK_SECRET=${STAGING_STRIPE_WEBHOOK_SECRET}
OPENAI_API_KEY=${STAGING_OPENAI_API_KEY}

# Twilio Settings
TWILIO_ACCOUNT_SID=${STAGING_TWILIO_ACCOUNT_SID}
TWILIO_AUTH_TOKEN=${STAGING_TWILIO_AUTH_TOKEN}

# JWT 
JWT_SECRET=${STAGING_JWT_SECRET}
JWT_EXPIRATION=24h

# Google Sheets Settings
GOOGLE_SHEETS_PRIVATE_KEY=${STAGING_GOOGLE_SHEETS_PRIVATE_KEY}
GOOGLE_SHEETS_CLIENT_EMAIL=${STAGING_GOOGLE_SHEETS_CLIENT_EMAIL}
GOOGLE_SHEETS_DOC_ID=${STAGING_GOOGLE_SHEETS_DOC_ID}

# AWS Settings
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=${AWS_ACCESS_KEY_ID}
AWS_SECRET_ACCESS_KEY=${AWS_SECRET_ACCESS_KEY}

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Voice Configuration
DEFAULT_VOICE=alloy
DEFAULT_MODEL=turbo
DEFAULT_TEMPERATURE=0.7
DEFAULT_FROM_NUMBER=+15555555555

# DynamoDB Table Prefix
DYNAMODB_TABLE_PREFIX=ailevelup-phone-call-mcp-staging

# Service Name
SERVICE_NAME=ailevelup-phone-call-mcp

# Feature Flags
ENABLE_AUTO_TOPUP=false
ENABLE_WEBHOOKS=true
ENABLE_ANALYTICS=true

# Logging
LOG_LEVEL=info 